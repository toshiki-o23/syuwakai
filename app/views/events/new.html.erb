<%= javascript_pack_tag 'events' %>

<% if @event.errors.any? %>
  <%= @event.errors.full_messages.each do |message| %>
    <%= message %>
  <% end %>
<% end %>

<%= form_with model: @event, url: '/events', method: :post, local: true do |f| %>

    <%= render 'partial/event_form', f: f, new_flag: true %>

<% end %>
