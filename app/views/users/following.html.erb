<%= @user.name %>のフォロー一覧
<% @users.each do |user| %>
  <% if user.email.present? %>
    <table>
      <tr>
        <td><%= link_to user.name, user_path(user) %></td>
      </tr>
    </table>
  <% else %>
    <p>フォローしていません</p>
  <% end %>
<% end %>

<%= link_to 'フォロワー一覧', user_follower_path(@user.id) %>
